@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix ui: <http://lv2plug.in/ns/extensions/ui#> .

<https://www.jahnichen.de/plugins/lv2/BAmp#GUI>
        a ui:X11UI ;
        ui:binary <BAmp_GUI.so> ;
        lv2:extensionData ui:idleInterface , ui:resize ;
	lv2:optionalFeature ui:resize ;
        lv2:requiredFeature ui:idleInterface .

<https://www.jahnichen.de/plugins/lv2/BAmp> a lv2:Plugin ;
  a lv2:Plugin, lv2:AmplifierPlugin, doap:Project;
  doap:name "B.Amplifier";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  lv2:binary <BAmp.so> ;
  lv2:microVersion 1 ;
  lv2:minorVersion 0 ;
  ui:ui <https://www.jahnichen.de/plugins/lv2/BAmp#GUI> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 0 ;
    lv2:symbol "gain" ;
    lv2:name "Gain";
    lv2:default 0.0 ;
    lv2:minimum -90.0 ;
    lv2:maximum 24.0 ;
  ] , [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 2 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] .
